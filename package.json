{
	"name": "guerra-website-mods",
	"version": "1.0.0",
	"description": "Website mods for Fillipo Guerra",
	"license": "UNLICENSED",
	"type": "module",
	"author": "Max Kohler<hello@maxkohler.com>",
	"main": "index.js",
	"scripts": {
		"coverage": "vitest --coverage",
		"build:rollup": "npx rollup -c",
		"watch:rollup": "npx rollup -w -c",
		"build:eleventy": "yarn exec eleventy --input=views",
		"watch:eleventy": "yarn exec eleventy --serve --quiet --input=views",
		"start": "run-p 'watch:*'",
		"build": "run-s 'build:*'",
		"test": "vitest"
	},
	"devDependencies": {
		"@11ty/eleventy": "^3.1.2",
		"@guanghechen/rollup-plugin-copy": "^6.0.9",
		"@rollup/plugin-image": "^3.0.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@rollup/plugin-typescript": "^12.3.0",
		"@vitest/coverage-v8": "4.0.18",
		"csv-parse": "^6.1.0",
		"npm-run-all": "^4.1.5",
		"rollup-plugin-serve": "^3.0.0",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"vitest": "^4.0.18"
	},
	"packageManager": "yarn@4.12.0"
}
